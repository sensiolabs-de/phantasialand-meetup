{
    "name": "christian/meetup.api",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "AppBundle\\": "src/AppBundle"
        },
        "classmap": [
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "guzzlehttp/psr7": "^1.4",
        "incenteev/composer-parameter-handler": "^2.0",
        "php-http/curl-client": "^1.7",
        "php-http/httplug-bundle": "^1.7",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/asset": "^3",
        "symfony/flex": "^1.0",
        "symfony/form": "^3",
        "symfony/lts": "^3",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/security-bundle": "^3",
        "symfony/serializer": "^3",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/templating": "^3",
        "symfony/translation": "^3",
        "symfony/twig-bundle": "^3",
        "symfony/validator": "^3",
        "symfony/workflow": "^3",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/debug-bundle": "^3",
        "symfony/dotenv": "^3",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/web-profiler-bundle": "^3",
        "symfony/web-server-bundle": "^3"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
